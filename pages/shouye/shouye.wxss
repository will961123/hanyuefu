/* pages/shouye/shouye.wxss */

page {
  width: 100%;
  height: auto;
  /* background-color: #a42218; */
  background-color: #b60005;
}

:-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}

.shouye {
  overflow: hidden;
  padding: 40rpx 0rpx;
  /* padding: 40rpx 30rpx; */
}

/* 通用 */

.bra6 {
  border-radius: 6rpx;
}

.mb30 {
  margin-bottom: 30rpx;
}

.elli {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* 轮播代码 */

.imgSwiper {
  width: 100%;
  height: 198px;
  border-radius: 8rpx;
  overflow: hidden;
}

.imgSwiper image {
  width: 100%;
  height: 100%;

  border-radius: 8rpx;
}

.imgSwiper .demo-text-1 {
  position: relative;
  align-items: center;
  justify-content: center;
  /* background-color: #1aad19; */
  color: #fff;
  font-size: 36rpx;
  border-radius: 8rpx;

  box-sizing: border-box;
  padding: 0 30rpx;
}

.imgSwiper .demo-text-2 {
  position: relative;
  align-items: center;
  justify-content: center;
  background-color: #2782d7;
  color: #fff;
  font-size: 36rpx;
}

.imgSwiper .demo-text-3 {
  position: relative;
  align-items: center;
  justify-content: center;
  background-color: rgb(243, 230, 55);
  color: #fff;
  font-size: 36rpx;
}

/* free-vessel 中间模块 */

.free-vessel {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-start;
  flex-wrap: nowrap;
  align-content: center;
  padding: 16px 30rpx;
  background: #f5e5e5;
  border-radius: 8rpx;
}

.free-vessel view {
  /* width: 215rpx;
  height: 215rpx; */
  /* background: orange; */
  text-align: center;
}

.free-vessel view image {
  width: 70rpx;
  height: 70rpx;
}

.free-vessel view text {
  font-size: 24rpx;
  font-weight: 700;
}

/* videoEnter */

.videoEnter {
  height: 80px;
  border-radius: 8rpx;
  overflow: hidden;
}

.videoEnter image {
  width: 100%;
  height: 100%;
}

/* 公告start */

.notice {
  height: 40px;
  line-height: 40px;
  /* padding: 10rpx 0; */
  background-color: #f5e5e5;
  font-size: 24rpx;
  color: rgb(10, 10, 10);
  overflow: hidden;
  border-radius: 8rpx;
  font-weight: 700;
}

.icontongzhi {
  position: absolute;
  margin: 0rpx 30rpx;
  color: #9d2515;
}

.swiper-notice {
  font-size: 28rpx;
  color: #fa6016;
}

.swiper-container {
  margin-left: 70rpx;
  width: 100%;
  height: 100%;
}

/* 楼盘信息 */

.houseInfo>.title {
  /* border-bottom: 1px solid #f5f5f9; */
  padding: 20rpx 30rpx 12rpx 30rpx;
  color: #333;
  background-color: #f8e6e6;
  border-top-left-radius: 6rpx;
  border-top-right-radius: 6rpx;
}

.houseInfo>.title text {
  font-size: 36rpx;
  font-weight: bold;
}

.houseInfo .box {
  background: #f8e6e6;
  margin-bottom: 30rpx;
  padding: 30rpx 30rpx;
  position: relative;
  border-radius: 8rpx;
  overflow: hidden;
}

.houseInfo .boxTop {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-start;
  flex-wrap: nowrap;
  align-content: center;
}

.houseInfo .tlr {
  border-top-left-radius: 0;
  border-top-right-radius: 0;
}

.houseInfo .boxTop>view {
  height: 215rpx;
}

.houseInfo .boxTop .img {
  /* width: 260rpx; */

  position: relative;
  width: 146px;
  /* flex: 2; */
  /* background: rgb(252, 179, 22); */
}

.houseInfo .boxTop .img .saling {
  position: absolute;
  left: 8px;
  font-size: 20rpx;
  color: white;
  background-color: #ac2118;
  width: 60rpx;
  text-align: center;
  padding: 2rpx 0 4rpx;
  top: 8px;
}

.houseInfo .boxTop .img image {
  /* width: 100%; */
  width: 146px;
  height: 100%;
}

.houseInfo .boxTop .info {
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
  flex-wrap: nowrap;
  align-content: center;
  /* background: white; */
  color: rgb(15, 15, 15);
  margin-left: 15rpx;
}

.houseInfo .boxTop .info>view {
  width: 340rpx;
}

.houseInfo .boxBottom {
  display: flex;
  /* flex-direction: row; */
  justify-content: space-between;
  /* align-items: flex-start; */
  align-items: center;
  flex-wrap: nowrap;
  align-content: center;
  /* padding: 8rpx 40rpx; */
  height: 30px;
  padding: 0 22rpx;
  margin-top: 16rpx;
  font-size: 24rpx;
  background: #f4dfde;
  color: #e7675c;
  box-sizing: border-box;
  border-radius: 8rpx;
  overflow: hidden;
}

/* .houseInfo .boxBottom .left {
  width: 450rpx;
} */


.houseInfo .boxBottom .newflex {
  display: flex;
  align-items: center;
}

.houseInfo .boxBottom .iconhongbao {
  /* position: absolute;
  left: 30rpx;
  margin-top: -6rpx; */
  margin-right: 10rpx;
  font-size: 34rpx;

}

.houseInfo .boxBottom .iconyoujiantou-tianchong {
  /* position: absolute;
  right: 30rpx;
  margin-top: -6rpx; */
  margin-left: 14rpx;
  font-size: 22rpx;
  margin-top: 2px;

}

.louPanName {
  font-size: 32rpx;
  color: black;
  font-weight: 700;
}

.projectLocation {
  font-size: 26rpx;
  margin-top: -8rpx;
}

.projectPrice {
  color: #e14744;
  font-weight: 600;
  font-size: 28rpx;
}

.projectInfo {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-start;
  flex-wrap: nowrap;
  align-content: center;
  font-size: 28rpx;
}

.fitmentType {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-start;
  flex-wrap: nowrap;
  font-size: 20rpx;
}

.fitmentType text {
  border: 1px solid #7c828a;
  padding: 0rpx 10rpx 2rpx;
  border-radius: 6rpx;
  box-sizing: border-box;
  color: rgb(100, 95, 95);
}

.noData {
  padding: 10rpx;
  text-align: center;
  font-size: 24rpx;
  color: white;
}

/* 搜索框样式 */

.search {
  display: flex;
  /* flex-direction: row;
  justify-content: space-between; */
  align-items: center;
  /* height: 60rpx; */
  /* background-color: whitesmoke;
  border: 1rpx solid whitesmoke; */
  background: #fff;
  font-size: 24rpx;
  border-radius: 8rpx;
